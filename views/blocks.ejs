<div class="row clearfix" ng-controller="blocksCtrl">
  <div class="col-lg-12 clearfix">
    <div class="block_s">
      <table class="table table-striped table-hover">
        <thead>
        <tr class='info'>
          <th style='vertical-align: middle;text-align: center;'>高度</th>
          <th style='vertical-align: middle;text-align: center;'>时间</th>
          <th style='vertical-align: middle;text-align: center;'>交易数</th>
          <!--<th style='vertical-align: middle;text-align: center;'>费用</th>-->
          <th class="hidden-xs" style='vertical-align: middle;text-align: center;'>共识地址</th>
          <th class="hidden-xs" style='vertical-align: middle;text-align: center;'>版本</th>
        </tr>
        </thead>
        <tbody>

        <tr ng-repeat="ss in blockList.docs">
          <td class="col-md-1" style='vertical-align: middle;text-align: center;'><a href="/block/{{ss.height}}">{{ss.height}}</a> </td>
          <td class="col-md-2" style='vertical-align: middle;text-align: center;'>{{ss.time*1000 | date:'yyyy-MM-dd HH:mm:ss'}}</td>
          <td class="col-md-1" style='vertical-align: middle;text-align: center;'>{{ss['txCount']}}</td>
          <!--<td class="col-md-1" style='vertical-align: middle;text-align: center;'>{{ss['cost']}}</td>-->
          <td class="col-md-2 hidden-xs" style='vertical-align: middle;text-align: center;'>{{ss['consensusAddress']}}</td>
          <td class="col-md-1 hidden-xs" style='vertical-align: middle;text-align: center;'>{{ss['version']}}</td>
        </tr>
        </tbody>
      </table>
    </div>
  </div>
  <div class="col-lg-12 clearfix">
    <div align="center">
      <ul class="pagination" id='block'>
      </ul>
    </div>
  </div>
</div>